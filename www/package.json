{
  "name": "@tlowerison/tilings",
  "version": "0.1.0",
  "author": "Trey Lowerison",
  "description": "Tilings",
  "main": "./src/index.tsx",
  "scripts": {
    "build": "rm -rf ./dist; NODE_ENV=production webpack",
    "restart:local": "yarn build && yarn start:nginx:local",
    "start": "webpack serve",
    "start:nginx:local": "yarn stop:nginx:local; ROOT=$PWD/dist envsubst < ../nginx/nginx.conf.template | sed -e 's/ยง/$/g' > ../nginx/nginx.conf && nginx -c $PWD/../nginx/nginx.conf && brew services restart nginx",
    "stop:nginx:local": "brew services stop nginx; lsof -i :80 | grep nginx | sed 's/nginx *//' | sed 's/ .*//' | xargs -n1 kill -9"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "client": "./pkg",
    "preact": "^10.5.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/sass": "^1.16.0",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.1",
    "react-refresh": "^0.10.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
